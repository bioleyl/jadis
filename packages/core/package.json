{
  "name": "@jadis/core",
  "version": "0.1.1",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.mjs",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/cjs/index.js",
      "import": "./dist/esm/index.mjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build:esm": "tsc -p tsconfig.esm.json && npm run rename:esm",
    "build": "npm run build:cjs && npm run build:esm",
    "clean": "rimraf dist",
    "rename:esm": "node ../../scripts/rename-js-to-mjs.mjs",
    "prepack": "npm run clean && npm run build"
  },
  "keywords": [],
  "author": "Leal Bioley <lb@syware.ch>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/bioleyl/jadis.git"
  },
  "bugs": {
    "url": "https://github.com/bioleyl/jadis/issues"
  },
  "description": "Jadis is a minimal JavaScript library for building web interfaces using native Web Components.",
  "dependencies": {
    "@jadis/helpers": "^1.0.0"
  }
}

{
  "private": true,
  "packageManager": "npm@10.8.2",
  "engines": {
    "npm": ">=10.0.0"
  },
  "engineStrict": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "docs:dev": "npm run dev --workspace=@jadis/docs",
    "docs:build": "npm run build --workspace=@jadis/docs",
    "docs:preview": "npm run preview --workspace=@jadis/docs",
    "publish:core": "npm publish --workspace=packages/core --access public",
    "publish:create": "npm publish --workspace=packages/create --access public",
    "test": "npm run test --workspaces"
  },
  "name": "jadis",
  "version": "1.0.0",
  "devDependencies": {
    "prettier": "3.6.2"
  }
}